{
  "documentationUrl": "https://docs.airbyte.io/integrations/destinations/bigquery",
  "supportsIncremental": true,
  "supported_destination_sync_modes": ["overwrite", "append"],
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "S3 Destination Spec",
    "type": "object",
    "required": ["AuthMode", "AwsAccountId", "Region", "BucketName", "Prefix"],
    "additionalProperties": false,
    "properties": {
      "AwsAccountId": {
        "type": "string",
        "title": "Aws Account Id",
        "description": "target aws account id",
        "examples": ["111111111111"]
      },
      "Region": {
        "title": "Aws Region",
        "type": "string",
        "description": "Region name",
        "airbyte_secret": false
      },
      "AuthMode": {
        "title": "Authentication mode",
        "Description": "Is authentication user based or role based?",
        "type": "string",
        "examples": ["USER", "ROLE"]
      },
      "RoleArn": {
        "title": "Target Role Arn",
        "type": "string",
        "description": "Will assume this role to write data to s3",
        "airbyte_secret": false
      },
      "AccessKeyId": {
        "title": "Access Key Id",
        "type": "string",
        "description": "AWS User Access Key Id",
        "airbyte_secret": true
      },
      "SecretAccessKey": {
        "title": "Secret Access Key",
        "type": "string",
        "description": "Secret Access Key",
        "airbyte_secret": true
      },
      "BucketName": {
        "title": "S3 Bucket Name",
        "type": "string",
        "description": "Name of the bucket",
        "airbyte_secret": false
      },
      "Prefix": {
        "title": "Target S3 Bucket Prefix",
        "type": "string",
        "description": "S3 prefix",
        "airbyte_secret": false
      },
      "DatabaseName": {
        "title": "Database",
        "type": "string",
        "description": "Which database to use",
        "airbyte_secret": false
      }
    }
  }
}
